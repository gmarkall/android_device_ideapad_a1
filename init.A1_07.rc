service wpa_supplicant /system/bin/wpa_supplicant -Dwext -iwlan0 -c/data/misc/wifi/wpa_supplicant.conf
    socket wpa_wlan0 dgram 0660 wifi system
    disabled
    oneshot

service dhcpcd /system/bin/dhcpcd -ABKL wlan0
